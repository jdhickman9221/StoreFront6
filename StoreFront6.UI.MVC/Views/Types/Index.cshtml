@model IEnumerable<StoreFront6.DATA.EF.Type>

@{
    ViewBag.Title = "Index";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { @class = "btn" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TypeID }, new { @class = "btn" }) |
            @Html.ActionLink("Details", "Details", new { id=item.TypeID }, new { @class = "btn" }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TypeID }, new { @class = "btn" })
        </td>
    </tr>
}

</table>
